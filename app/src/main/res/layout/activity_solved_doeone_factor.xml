                </TextView>






                <TextView
                    android:id="@+id/anovamc2oneDOE"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:background="@drawable/rect"
                    android:layout_weight="125"
                    android:layout_marginStart="8dp"
