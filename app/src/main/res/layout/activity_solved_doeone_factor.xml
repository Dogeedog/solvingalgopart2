            app:layout_constraintTop_toBottomOf="@+id/tvICMeanDifValue"
            android:layout_marginTop="24dp"
            android:textColor="@color/white">

        </TextView>

        <LinearLayout
            android:id="@+id/LSDlinearlayout"
            android:layout_width="wrap_content"
